				format(g_Companies[companyid][COMPANY_NAME], MAX_COMPANY_NAME, g_CreatingCompany[playerid][COMPANY_CREATING_NAME]);
				format(g_Companies[companyid][COMPANY_OWNER], MAX_PLAYER_NAME, "Ninguem");
				g_Companies[companyid][COMPANY_TYPE] = g_CreatingCompany[playerid][COMPANY_CREATING_TYPE];
				g_Companies[companyid][COMPANY_VALUE] = g_CreatingCompany[playerid][COMPANY_CREATING_VALUE];
				g_Companies[companyid][COMPANY_HASOWNER] = false;
				g_Companies[companyid][COMPANY_ISCRASH] = false;

				GenerateCompanyPickup(companyid, false);
				SaveCompany(companyid, true);
			}
		}
	}
	if(dialogid == DIALOG_CRIAR_EMPRESA_NOME)
	{
		if (response)
			format(g_CreatingCompany[playerid][COMPANY_CREATING_NAME], MAX_COMPANY_NAME, inputtext);
		
		ShowCompanyCreateMenu(playerid, g_SelectedCompany[playerid]);
	}
	if(dialogid == DIALOG_CRIAR_EMPRESA_TIPO)
	{
		if (response)
			g_CreatingCompany[playerid][COMPANY_CREATING_TYPE] = listitem;
		
		ShowCompanyCreateMenu(playerid, g_SelectedCompany[playerid]);
	}
	if(dialogid == DIALOG_CRIAR_EMPRESA_VALOR)
	{
		if (response)
			g_CreatingCompany[playerid][COMPANY_CREATING_VALUE] = strval(inputtext);
		
		ShowCompanyCreateMenu(playerid, g_SelectedCompany[playerid]);
	}
	if(dialogid == DIALOG_EMP_ADMIN && response)
	{
		new stringDialog[400];
		new companyid = g_SelectedCompany[playerid];
		switch(listitem)
		{
			case 0:
			{
				format(stringDialog, sizeof(stringDialog), "Atual: %s", g_Companies[companyid][COMPANY_NAME]);
				ShowPlayerDialog(playerid, DIALOG_EMP_ADMIN_NOME, DIALOG_STYLE_INPUT, "Escolha o nome da empresa", stringDialog, "Confirmar", "Voltar");
			}
			case 1:
			{
				format(stringDialog, sizeof(stringDialog), "Atual: R$%d", g_Companies[companyid][COMPANY_VALUE]);
				ShowPlayerDialog(playerid, DIALOG_EMP_ADMIN_VALOR, DIALOG_STYLE_INPUT, "Escolha o valor da empresa", stringDialog, "Confirmar", "Voltar");
			}
			case 2, 3:
			{
				SendClientMessage(playerid, -1, "Essa função está desativada!");
				ShowCompanyAdminMenu(playerid, g_SelectedCompany[playerid]);
			}
			case 4:
				ShowPlayerDialog(playerid, DIALOG_EMP_ADMIN_POSICAO, DIALOG_STYLE_LIST, "Posicionamentos da empresa", "Entrega\nVenda\nFerramentas\nEscritório", "Confirmar", "Voltar");
			case 5:
			{
				format(stringDialog, sizeof(stringDialog), "Atual: %d", g_Companies[companyid][COMPANY_MAX_MATERIALS]);
				ShowPlayerDialog(playerid, DIALOG_EMP_ADMIN_MATERIAIS, DIALOG_STYLE_INPUT, "Escolha o limite de materiais da empresa", stringDialog, "Confirmar", "Voltar");
			}
			case 6:
			{
				g_Companies[companyid][COMPANY_ISCRASH] = !g_Companies[companyid][COMPANY_ISCRASH];
				GenerateCompanyPickup(companyid);
			}
		}
	}
}

public OnFilterScriptInit()
{
	for(new i=0; i < MAX_COMPANY; i++)
		LoadCompany(i);
}

public OnFilterScriptExit()
{
	for(new i=0; i < MAX_COMPANY; i++)
		SaveCompany(i);
	
	DestroyAllDynamic3DTextLabels();
	DestroyAllDynamicPickups();
}

public OnPlayerCommandText(playerid, cmdtext[]) {
	new idx;
	new cmd[256];
    new tmp[150];
	cmd = strtok(cmdtext, idx);

    // if(strcmp(cmd, "/comando", true) == 0) {
	// 	tmp = strtok(cmdtext, idx);
    //     if(!strlen(tmp))
    //     {
    //         SendClientMessage(playerid, -1, "Use: /comando [id]");
    //         return 1;
    //     }
    //     new id = strval(tmp);
	// 	return 1;
	// }

    if(strcmp(cmd, "/infoemp", true) == 0) {
		tmp = strtok(cmdtext, idx);
        if(!strlen(tmp)) return 1;

        new companyid = strval(tmp);

		if (IsPlayerInRangeOfPoint(playerid, 3.0, g_Companies[companyid][COMPANY_X], g_Companies[companyid][COMPANY_Y], g_Companies[companyid][COMPANY_Z]))
		{
			new companyType = g_Companies[companyid][COMPANY_TYPE];
			new location[MAX_ZONE_NAME];
			Get2DZone(location, g_Companies[companyid][COMPANY_X], g_Companies[companyid][COMPANY_Y], MAX_ZONE_NAME);
			new stringDialog[400];
			format(stringDialog, sizeof(stringDialog), "{80FF80}%s:", g_Companies[companyid][COMPANY_NAME]);
			format(stringDialog, sizeof(stringDialog), "%s{FFFFFF}\n\nEmpresa ID: %d\nLocal: %s\nDono: %s\nTipo: %s\nValor: R$ %d", stringDialog, companyid, location, g_Companies[companyid][COMPANY_OWNER], g_CompaniesTypes[companyType], g_Companies[companyid][COMPANY_VALUE]);
			strcat(stringDialog, "\n\n{1E90FF}INFO: {FFFFFF}Você só consegue entrar se a empresa estiver aberta.\n");
			if(g_Companies[companyid][COMPANY_INPUT_VALUE] > 0)
			{
				format(stringDialog, sizeof(stringDialog), "\n{1E90FF}INFO: {FFFFFF}Você pagará {CC0000}R$%d{FFFFFF} pela entrada.\n", g_Companies[companyid][COMPANY_INPUT_VALUE]);
			}
			ShowPlayerDialog(playerid, DIALOG_ENTRAR_EMPRESA, DIALOG_STYLE_MSGBOX, "{33CCFF}Propriedade", stringDialog, "Entrar", "Cancelar");
		}
		return 1;
	}

    if(strcmp(cmd, "/menuempresa", true) == 0) {
		tmp = strtok(cmdtext, idx);
        if(strlen(tmp))
		{
			new companyid = strval(tmp);

			if(companyid < 0 || companyid >= MAX_COMPANY || !g_Companies[companyid][COMPANY_ISLOADED])
			{
				SendClientMessage(playerid, -1, "Essa empresa não existe");
				return 1;
			}
			ShowCompanyAdminMenu(playerid, companyid);
			return 1;
		}

		new Float:empX, Float:empY, Float:empZ;
		new unloadedid = MAX_COMPANY;

		for (new i=0; i < MAX_COMPANY; i++)
		{
			if(g_Companies[i][COMPANY_ISLOADED])
			{
				empX = g_Companies[i][COMPANY_X];
				empY = g_Companies[i][COMPANY_Y];
				empZ = g_Companies[i][COMPANY_Z];
				if(IsPlayerInRangeOfPoint(playerid, 3, empX, empY, empZ))
				{
					ShowCompanyAdminMenu(playerid, i);
					return 1;
				}
			}
			else if(i < unloadedid)
				unloadedid = i;
		}

		if (unloadedid == MAX_COMPANY)
			return SendClientMessage(playerid, -1, "Foi atingido a quantidade máxima de empresas no servidor!");

		ShowCompanyCreateMenu(playerid, unloadedid, true);
		return 1;
	}

	if(strcmp(cmd, "/ir", true) == 0)
	{
		tmp = strtok(cmdtext, idx);
		if(!strlen(tmp))
		{
			SendClientMessage(playerid, -1, "USE: /ir [id]");
			return 1;
		}
		new Float:plocx,Float:plocy,Float:plocz;
		new plo;
		plo = ReturnUser(tmp);

		if (!IsPlayerConnected(plo))
			return 1;

		GetPlayerPos(plo, plocx, plocy, plocz);
		SetPlayerInterior(playerid,GetPlayerInterior(plo));
		SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(plo));

		SetPlayerPos(playerid,plocx,plocy+2, plocz);
		return 1;
	}

	if(strcmp(cmd, "/arma", true) == 0) {
		GivePlayerWeapon(playerid, 31, 400);
		return 1;
	}

	if(strcmp(cmd, "/minigun", true) == 0) {
		GivePlayerWeapon(playerid, 38, 900);
		return 1;
	}

	if(strcmp(cmd, "/delcars", true) == 0) {
		for(new i=0; i <= GetVehiclePoolSize(); i++)
		{
			if(IsValidVehicle(i))
			{
				DestroyVehicle(i);
			}
		}
		return 1;
	}

	if(strcmp(cmd, "/repair", true) == 0)
    {
        if (!IsPlayerInAnyVehicle(playerid))
        {
            return SendClientMessage(playerid, 0xFFFFFFFF, "You are not in a vehicle!");
        }

        RepairVehicle(GetPlayerVehicleID(playerid));

        SendClientMessage(playerid, 0xFFFFFFFF, "Your vehicle has been repaired!");
        return 1;
    }

	if(strcmp(cmd, "/motor", true) == 0) {
		if(!IsPlayerInAnyVehicle(playerid))
			return 1;
		new vehicleid = GetPlayerVehicleID(playerid);
		new engine, lights, alarm, doors, bonnet, boot, objective;
		GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);

		if(engine == 0)
			engine = 1;
		else
			engine = 0;
		SetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);

		return 1;
	}

	if(strcmp(cmd, "/farol", true) == 0) {
		if(!IsPlayerInAnyVehicle(playerid))
			return 1;
		new vehicleid = GetPlayerVehicleID(playerid);
		new engine, lights, alarm, doors, bonnet, boot, objective;
		GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);

		if(lights == 0)
			lights = 1;
		else
			lights = 0;
		SetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);

		return 1;
	}

	if(strcmp(cmd, "/createcar", true) == 0) {
		new Float:nextX, Float:nextY, Float:nextZ;

		tmp = strtok(cmdtext, idx);
        if(!strlen(tmp))
        {
            SendClientMessage(playerid, -1, "Use: /createcar [id]");
            return 1;
        }
        new carModel = strval(tmp);

        GetPlayerPos(playerid, nextX, nextY, nextZ);
		CreateVehicle(carModel, nextX, nextY, nextZ, 82.2873, -1, -1, -1);
		return 1;
	}

    return 0;
}

