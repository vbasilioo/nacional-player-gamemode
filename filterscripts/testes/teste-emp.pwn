stock GenerateCompanyPickup(companyid, bool:isLoaded=true)
{
	new string[200];
	new Float:posX = g_Companies[companyid][COMPANY_X];
	new Float:posY = g_Companies[companyid][COMPANY_Y];
	new Float:posZ = g_Companies[companyid][COMPANY_Z];
	new pickupid = !g_Companies[companyid][COMPANY_HASOWNER] ? 19524 : g_Companies[companyid][COMPANY_ISCRASH] ? 19522 : 19523;

	if(g_Companies[companyid][COMPANY_ISLOADED] && isLoaded)
	{
		DestroyDynamic3DTextLabel(g_Companies[companyid][COMPANY_3D_TEXT]);
		DestroyDynamicPickup(g_Companies[companyid][COMPANY_PICKUP]);
	}

	format(string, sizeof(string), "{FFFFFF}%s\n[ID %d]", g_Companies[companyid][COMPANY_NAME], companyid);
	g_Companies[companyid][COMPANY_3D_TEXT] = CreateDynamic3DTextLabel(string, COR_3DLABEL, posX, posY, posZ+0.5, 30.0);
	g_Companies[companyid][COMPANY_PICKUP] = CreateDynamicPickup(pickupid, 1, posX, posY, posZ);
}

stock bool:LoadCompany(companyid)
{
	new arquivo[40];
	format(arquivo, sizeof(arquivo), COMPANY_PATH, companyid);

	if(!DOF2_FileExists(arquivo))
	{
		printf("Houve uma tentativa fracassada de carregar a empresa [id:%d]", companyid);
		g_Companies[companyid][COMPANY_ISLOADED] = false;
		return false;
	}

	DOF2_GetStringEx(arquivo, "Name", g_Companies[companyid][COMPANY_NAME], MAX_COMPANY);
	DOF2_GetStringEx(arquivo, "Owner", g_Companies[companyid][COMPANY_OWNER], MAX_PLAYER_NAME);
	g_Companies[companyid][COMPANY_X] = DOF2_GetFloat(arquivo, "X");
	g_Companies[companyid][COMPANY_Y] = DOF2_GetFloat(arquivo, "Y");
	g_Companies[companyid][COMPANY_Z] = DOF2_GetFloat(arquivo, "Z");
	g_Companies[companyid][COMPANY_VALUE] = DOF2_GetInt(arquivo, "Value");
	g_Companies[companyid][COMPANY_CASH] = DOF2_GetInt(arquivo, "Cash");
	g_Companies[companyid][COMPANY_ISLOADED] = true;
	GenerateCompanyPickup(companyid, false);

	printf("Empresa carregada [id:%d]", companyid);
	return true;
}

stock bool:SaveCompany(companyid, bool:force=false)
{
	new arquivo[40];
	format(arquivo, sizeof(arquivo), COMPANY_PATH, companyid);

	if(!DOF2_FileExists(arquivo))
	{
		if(g_Companies[companyid][COMPANY_ISLOADED] || force)
		{
			DOF2_CreateFile(arquivo);
		}
		else
		{
			printf("Houve uma tentativa fracassada de salvar a empresa [id:%d]", companyid);
			return false;
		}
	}

